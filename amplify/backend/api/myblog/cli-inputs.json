{
  "version": 1,
  "paths": {
    "/posts/{id}": {
      "name": "/posts/{id}",
      "lambdaFunction": "myblogPosts",
      "permissions": {
        "setting": "open"
      }
    },
    "/images": {
      "name": "/images",
      "lambdaFunction": "myblogImages",
      "permissions": {
        "setting": "open"
      }
    },
    "/category/{id}": {
      "name": "/category/{id}",
      "lambdaFunction": "myblogCategory",
      "permissions": {
        "setting": "open"
      }
    },
    "/postlists/{id}": {
      "name": "/postlists/{id}",
      "lambdaFunction": "myblogAllPosts",
      "permissions": {
        "setting": "open"
      }
    }
  }
}
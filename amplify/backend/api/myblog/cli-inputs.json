{
  "version": 1,
  "paths": {
    "/postlists/{id}": {
      "name": "/postlists/{id}",
      "lambdaFunction": "myblogGetUserPostLists",
      "permissions": {
        "setting": "open"
      }
    },
    "/posts/{id}": {
      "name": "/posts/{id}",
      "lambdaFunction": "myblogPosts",
      "permissions": {
        "setting": "open"
      }
    },
    "/images": {
      "name": "/images",
      "lambdaFunction": "myblogImages",
      "permissions": {
        "setting": "open"
      }
    }
  }
}